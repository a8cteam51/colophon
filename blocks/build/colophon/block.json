{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "team51/colophon",
  "version": "1.1.0",
  "title": "Colophon",
  "category": "widgets",
  "icon": "admin-links",
  "description": "Having set up footer links in many partner sites, this aims to simplify the deployment of each, by using a more consistent api.",
  "supports": {
    "html": false
  },
  "textdomain": "team51",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "separator": {
      "type": "string",
      "default": " | "
    },
    "pressableLink": {
      "type": "string",
      "default": "__"
    },
    "wpComLink": {
      "type": "string",
      "default": "__"
    },
    "hasWrapper": {
      "type": "boolean",
      "default": false
    },
    "wrapperClassName": {
      "type": "string",
      "default": "colophon__wrapper"
    }
  }
}