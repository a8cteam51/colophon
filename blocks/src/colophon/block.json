{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "wpcomsp-scaffold/foobar",
    "version": "0.1.0",
    "title": "FooBar Block",
    "category": "widgets",
    "icon": "smiley",
    "description": "Example block scaffolded with Create Block tool.",
    "supports": {
        "html": false
    },
    "textdomain": "wpcomsp-scaffold",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css",
    "attributes": {
        "separator": {
            "type": "string",
            "default": " | "
        },
        "pressableLink": {
            "type": "string",
            "default": "__"
        },
        "wpComLink": {
            "type": "string",
            "default": "__"
        },
        "hasWrapper": {
            "type": "boolean",
            "default": false
        },
        "wrapperClassName": {
            "type": "string",
            "default": ""
        }
    }
}